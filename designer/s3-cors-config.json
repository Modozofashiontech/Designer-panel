{
  "CORSRules": [
    {
      "AllowedHeaders": ["*"],
      "AllowedMethods": ["GET", "HEAD", "PUT", "POST"],
      "AllowedOrigins": [
        "http://localhost:3000",
        "http://localhost:5000",
        "https://your-production-domain.com",
        "*"
      ],
      "ExposeHeaders": [
        "x-amz-server-side-encryption",
        "x-amz-request-id",
        "x-amz-id-2",
        "ETag",
        "Content-Type",
        "Content-Length"
      ],
      "MaxAgeSeconds": 86400
    }
  ]
}
